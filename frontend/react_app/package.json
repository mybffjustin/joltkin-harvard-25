{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "mybffjustin",
    "email": "justinh@alumni.harvard.edu"
  },
  "engines": {
    "node": ">=20.11",
    "npm": ">=9.0"
  },
  "scripts": {
    "predev": "algokit project link --all || echo 'skip (algokit not found)'",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "jest --coverage --passWithNoTests",
    "playwright:test": "playwright test",
    "analyze": "cross-env VITE_ANALYZE=true vite build && (open dist/stats.html || xdg-open dist/stats.html || true)"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "@perawallet/connect": "^1.4.2",
    "@txnlab/use-wallet": "^4.3.1",
    "@txnlab/use-wallet-react": "^4.3.1",
    "@walletconnect/sign-client": "^2.21.8",
    "algosdk": "^3.4.0",
    "buffer": "^6.0.3",
    "daisyui": "^4.0.0",
    "notistack": "^3.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^5.0.0",
    "@playwright/test": "^1.47.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/qrcode": "1.5.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "playwright": "^1.47.0",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^7.1.4",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
    "eslintConfig": {
    "extends": [
      "react-app/jest",
      "react-app"
    ]
  },
    "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "ws@>7.0.0 <7.5.9": "7.5.10"
  }
}
